{
  "id": "toggl-app",
  "name": "Toggl",
  "version": "0.0.3",
  "description": "Integrate data from Toggl time tracker into Fibery",
  "authentication": [
    {
      "description": "Please provide toggl authentication",
      "name": "Token",
      "id": "key",
